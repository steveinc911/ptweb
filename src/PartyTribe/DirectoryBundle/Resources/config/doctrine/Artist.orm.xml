<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PartyTribe\DirectoryBundle\Entity\Artist" table="artist">
    <id name="artistId" type="integer" column="artist_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="true"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
    <field name="artistTypeTags" type="string" column="artist_type_tags" length="255" nullable="true"/>
    <field name="genreTags" type="string" column="genre_tags" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
